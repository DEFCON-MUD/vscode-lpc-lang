{
  "name": "vscode-lpc-lang",
  "displayName": "LPC Language",
  "description": "LPmud C Language syntax highlighting for the FLUFFOS branch of LPmud drivers.",
  "version": "0.1.0",
  "publisher": "undeadfish",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "homepage": "https://github.com/undeadfish/vscode-lpc-lang/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/undeadfish/vscode-lpc-lang.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "lpc",
        "aliases": [ "LPC", "lpc" ],
        "extensions": [ ".c", ".h" ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lpc",
        "scopeName": "source.lpc",
        "path": "./syntaxes/lpc.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "lpc",
        "path": "./snippets/lpc-snippets.json"
      }
    ]
  }
}
